<?xml version="1.0"?>
<registry>

  <records prefix="plone.resources/mockup-patterns-handsontable"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++plone++handsontable/patterns/handsontable/pattern.js</value>
      <value key="css">
        <element>++plone++handsontable/less/handsontable.less</element>
      </value>
  </records>

  <records prefix="plone.resources/mockup-bundle-handsontable"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++plone++handsontable/js/bundles/handsontable.js</value>
  </records>

  <records prefix="plone.resources/handsontable"
           interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++plone++handsontable/bower_components/handsontable/dist/handsontable.full.min.js</value>
  </records>

  <records prefix="plone.bundles/handsontable"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>mockup-bundle-handsontable</element>
    </value>
    <value key="depends">plone</value>
    <value key="enabled">True</value>
    <value key="compile">True</value>
  </records>

</registry>
